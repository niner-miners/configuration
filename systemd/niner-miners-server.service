[Unit]
After=syslog.target
After=network.target

[Service]
User=niner-miners
Group=niner-miners
ExecStart=/home/niner-miners/core/actions/server
ExecStop=/home/niner-miners/core/actions/exec stop
ExecStop=sleep 2s
Restart=always
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=niner-miners-server

[Install]
WantedBy=multi-user.target
